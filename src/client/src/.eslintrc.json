{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-props-no-spreading": "off",
        "import/prefer-default-export": "off",
        "react/jsx-filename-extension": "off",
        "linebreak-style": "off"
    },
    "settings": {
      "import/core-modules": [ "electron" ]
    }
}
